{
  "ci": {
    "collect": {
      "startServerCommand": "yarn preview --port 4173",
      "startServerReadyPattern": "Local",
      "url": [
        "http://localhost:4173/"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.6
          }
        ]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lhci_reports"
    }
  }
}
